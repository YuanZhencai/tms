				            <f:selectItem itemLabel="#{msgs.itemLbl_afceFlag_N}" itemValue="N" />  
				            <f:selectItem itemLabel="#{msgs.itemLbl_afceFlag_Y}" itemValue="Y" />
       					</p:selectOneRadio>
       					<h:outputLabel value="" />
