public class AccountBankSelectVo extends IdModel implements Serializable {

	private static final long serialVersionUID = 1L;
	
	/** 总行信息 */
	private Bank topBank;
	/** 银行编码 */
	private String bankCode;
	/** BSB代码 */
	private String bsbCode;
	/** 联行号 */
	private String unionBankNo;
	/** 银行名称  */
	private String bankName;
	
	public AccountBankSelectVo(){
		topBank = new Bank();
	}

	public Bank getTopBank() {
		return topBank;
	}

	public void setTopBank(Bank topBank) {
		this.topBank = topBank;
	}

	public String getBankCode() {
		return bankCode;
	}

	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}

	public String getBsbCode() {
		return bsbCode;
	}

	public void setBsbCode(String bsbCode) {
		this.bsbCode = bsbCode;
	}

	public String getUnionBankNo() {
		return unionBankNo;
	}

	public void setUnionBankNo(String unionBankNo) {
		this.unionBankNo = unionBankNo;
	}

	public String getBankName() {
		return bankName;
	}

	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	
	
	
	
}
